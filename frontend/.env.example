# Environment Variables Template
# Copy this file to .env and fill in your actual values
# This file shows all available environment variables

# App Configuration
APP_ENV=
NODE_ENV=
PORT=

# Public variables (accessible in browser)
# Must be prefixed with VITE_ for Vite to expose them
VITE_APP_NAME=
VITE_API_URL=
VITE_PUBLIC_URL=

# Private variables (server-side only)
SESSION_SECRET=
API_TOKEN=

# Development settings
DEBUG=

# Test settings (for test environment)
TEST_MODE=

# Build optimization
VITE_BUILD_MODE=
