{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm ci",
  "framework": "remix",
  "env": {
    "VITE_NODE_ENV": "VERCEL_ENV",
    "VITE_VERCEL_URL": "VERCEL_URL"
  },
  "git": {
    "deploymentEnabled": {
      "staging": true
    }
  },
  "github": {
    "autoAlias": true
  },
  "alias": [
    {
      "domain": "xai-community-git-staging-ktsfrank-navercoms-projects.vercel.app",
      "branch": "staging"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/entry.server.tsx"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/build/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "regions": ["icn1"]
}